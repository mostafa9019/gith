{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/images/*.{png,jpg,gif,svg}",
      "/css/*",
      "/js/*",
      "*.{css,scss,js,json}"
    ]
  },
  "mimeTypes": {
    ".json": "application/json",
    ".woff": "application/font-woff",
    ".woff2": "application/font-woff2"
  },
  "globalHeaders": {
    "Cache-Control": "no-cache"
  },
  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": ["anonymous"]
    }
  ]
}
